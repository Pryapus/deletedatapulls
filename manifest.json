{
    "manifest_version": 3,
    "name": "DataPull Deletion",
    "version": "1.0",
    "permissions": [
      "<all_urls>",
      "tabs",
      "cookies"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "https://www.analytics-toolkit.com/*"
    ]
  }
  