{
    "manifest_version": 2,
    "name": "DataPull Deletion",
    "version": "1.0",
    "permissions": [
      "webRequest",
      "<all_urls>",
      "tabs",
      "cookies"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
  }
  